{
    "name": "php-ai/gemini-generative-ai",
    "description": "Laravel-friendly PHP Package for Google Gemini Generative AI (including image & text)",
    "keywords": ["gemini", "generative", "ai", "php", "sdk", "laravel", "google", "vision"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpAi\\Gemini\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Zero-Asif",
            "email": "asifuzzamanasif0001@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "google/auth": "^1.47",
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PhpAi\\Gemini\\Providers\\GeminiServiceProvider"
            ],
            "aliases": {
                "Gemini": "PhpAi\\Gemini\\Facades\\Gemini"
            }
        }
    },
    "require-dev": {
        "illuminate/support": "^12.19",
        "illuminate/contracts": "^12.19",
        "illuminate/config": "^12.19"
    },
    "prefer-stable": true
}
